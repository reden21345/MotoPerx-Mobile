{
  "expo": {
    "name": "Motoperx",
    "slug": "MotoPerx-Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/motoperx-logo.jpeg",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/motoperx-logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/motoperx-logo.jpeg",
        "backgroundColor": "#000000"
      },
      "package": "com.reden21345.MotoPerxMobile",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "VIBRATE"
      ]
    },
    "scheme": "motoperx",
    "deepLinking": true,
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bf72bc7c-3253-4dfb-b5d4-d331384213af"
      }
    },
    "owner": "motoperx",
    "plugins": [
      "expo-barcode-scanner",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MotoPerx to use your location"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ]
  }
}